{% extends 'base.html.twig' %}

{% block title %}PhotoUser index{% endblock %}

{% block body %}
    <h1>PhotoUser index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Photo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for photo_user in photo_users %}
            <tr>
                <td>{{ photo_user.id }}</td>
                <td>{{ photo_user.photo }}</td>
                <td>
                    <a href="{{ path('photo_user_show', {'id': photo_user.id}) }}">show</a>
                    {# <a href="{{ path('photo_user_edit', {'id': photo_user.id}) }}">edit</a> #}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {# <a href="{{ path('photo_user_new') }}">Create new</a> #}
{% endblock %}
